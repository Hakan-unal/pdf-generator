{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/hello.ts": {
            "memory": 3008,
            "maxDuration": 60
        },
        "api/another.ts": {
            "memory": 1024,
            "maxDuration": 30
        },
        "api/product": {
            "memory": 1024,
            "maxDuration": 30
        },
        "api-docs": {
            "memory": 1024,
            "maxDuration": 30
        }
    }
}